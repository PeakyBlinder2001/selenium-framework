<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Parallel Suite" parallel="methods" thread-count="3">

    <listeners>
        <listener class-name="reports.ExtentTestListener"/>
    </listeners>

    <test name="Parallel Smoke Tests">
        <classes>
            <class name="tests.SmokeTest"/>
        </classes>
    </test>

</suite>
